{
  "name": "seed-smith",
  "version": "1.0.1",
  "description": "seed-smith - A CLI tool to seed databases from YAML or JSON files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "db-seed": "./bin/db-seed.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node dist/cli.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "prepublishOnly": "npm run build && npm run lint && npm test"
  },
  "keywords": [
    "database",
    "seed",
    "cli",
    "mongodb",
    "postgres",
    "postgresql",
    "development",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chaieeetanya433/seed-smith.git"
  },
  "bugs": {
    "url": "https://github.com/chaieeetanya433/seed-smith/issues"
  },
  "homepage": "https://github.com/chaieeetanya433/seed-smith#readme",
  "author": "Chaitanya Agarkar",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.3.0",
    "mongodb": "^5.8.1",
    "ora": "^5.4.1",
    "pg": "^8.11.3",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.4",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.6.0",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "eslint": "^8.49.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "bin"
  ]
}
